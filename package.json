{
  "name": "your-app-name",
  "version": "1.0.0",
  "description": "A MERN stack application with GraphQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:client": "cd client && npm start",
    "start:server": "nodemon server.js",
    "install:client": "cd client && npm install",
    "build:client": "cd client && npx vite build",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build": "npm run build:client && npm run start:server"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Tyler Yannes",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.19.2",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "mongodb": "^6.5.0",
    "mongoose": "^5.11.15"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
